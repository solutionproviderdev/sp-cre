{
  "cli": {
    "version": ">= 15.0.13",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"android": {
				"buildType": "apk"
			}
		},
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
			"android": {
				"buildType": "apk"
			},
			"autoIncrement": true,
			"channel": "production"
		}
  },
  "submit": {
    "production": {}
  }
}
